<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--园loading
    circle_color 颜色
    circle_radius 半径
    circle_centerX circle_centerY 中心点坐标
    circle_duration 动画时长
    -->
    <declare-styleable name="CircleLoading">
        <attr name="circle_color" format="color"></attr>
        <attr name="circle_radius" format="float"></attr>
        <attr name="circle_centerX" format="float"></attr>
        <attr name="circle_centerY" format="float"></attr>
        <attr name="circle_duration" format="integer"></attr>
        <attr name="circle_p_with" format="float"></attr>
    </declare-styleable>
    <!--双圆环Loading
    circle_color 颜色
    circle_radius 半径
    circle_centerX circle_centerY 中心点坐标
    circle_duration 动画时长
    -->
    <declare-styleable name="DoubleCircleLoading">
        <attr name="circle_d_color" format="color" />
        <attr name="circle_ani_color" format="color" />
        <attr name="circle_d_radius" format="float" />
        <attr name="circle_d_centerX" format="float" />
        <attr name="circle_d_centerY" format="float" />
        <attr name="circle_d_duration" format="integer" />
        <attr name="circle_d_p_with" format="float" />
    </declare-styleable>

    <!--
    PrinterTextView 打印字体
     printer_duration 动画时长
     printer_show_message 要打印的字体
    -->

    <declare-styleable name="PrinterTextView">
        <attr name="printer_duration" format="integer"></attr>
        <attr name="printer_show_message" format="string"></attr>
    </declare-styleable>

    <!--
    DragBubbleView 仿qq聊天界面提示
    dbv_show_num 要显示的数字
    dbv_ball_radius 小球的半径
    dbv_ball_color 小球颜色
    dbv_num_color 文字颜色
    dbv_ball_centerX dbv_ball_centerY  初始中心点坐标
    -->
    <declare-styleable name="DragBubbleView">
        <attr name="dbv_show_num" format="string"></attr>
        <attr name="dbv_ball_radius" format="float"></attr>
        <attr name="dbv_ball_color" format="color"></attr>
        <attr name="dbv_num_color" format="color"></attr>

        <attr name="dbv_ball_centerX" format="float"></attr>
        <attr name="dbv_ball_centerY" format="float"></attr>
    </declare-styleable>


    <declare-styleable name="DragBubbleQQView">
        <attr name="bubble_radius" format="dimension" />
        <attr name="bubble_color" format="color" />
        <attr name="bubble_text" format="string" />
        <attr name="bubble_textSize" format="dimension" />
        <attr name="bubble_textColor" format="color" />
    </declare-styleable>

    <!--
    lineColor 虚线的颜色
    titleTextColor 标题的颜色
    title 标题
    titleTextSize 标题的大小

    avgTitle 平均数显示的内容
    avgTitleTextSize 平均数 字体的大小
    avgTitleTextColor 平均数字体颜色
    bottomLineColor 底部线的颜色
    scrollLine 底部滑动条是否显示
    titleCenter 标题是否居中显示

    -->
    <declare-styleable name="barCharts">
        <attr name="lineColor" format="color" />
        <attr name="titleTextColor" format="color" />
        <attr name="title" format="string" />
        <attr name="titleTextSize" format="dimension" />

        <attr name="avgTitleTextSize" format="dimension" />
        <attr name="avgTitleTextColor" format="color" />
        <attr name="avgTitle" format="string" />

        <attr name="bottomLineColor" format="color" />

        <attr name="leftLabelTextSize" format="dimension" />
        <attr name="leftLabelTextColor" format="color" />


        <attr name="labelTextSize" format="dimension" />
        <attr name="labelTextColor" format="color" />

        <attr name="descriptionTextSize" format="dimension" />
        <attr name="descriptionTextColor" format="color" />
        <attr name="avgLineColor" format="color" />
        <attr name="dataTextSize" format="dimension" />
        <attr name="dataTextColor" format="color" />
        <attr name="leftTextSpace" format="dimension" />
        <attr name="topTextSpace" format="dimension" />
        <attr name="bottomTextSpace" format="dimension" />
        <attr name="barShowNumber" format="integer" />
        <attr name="isClickAnimation" format="boolean" />
        <attr name="scrollLine" format="boolean" />
        <attr name="titleCenter" format="boolean" />
    </declare-styleable>

</resources>